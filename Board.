#ifndef BOARD_H
#define BOARD_H

#include <vector>

using namespace std;

class Board {
private:
    vector<vector<int>> cells;

public:
    Board();

    void initialize();
    bool setValue(int row, int col, int value);
    int getValue(int row, int col);
    bool isComplete();
    bool isCellEmpty(int row, int col);
};

#endif // BOARD_H
